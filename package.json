{
  "name": "httpbin",
  "version": "0.0.1",
  "devDependencies": {
    "@openworkers/adapter-sveltekit": "^0.5.6",
    "@openworkers/workers-types": "^0.1.9",
    "@sveltejs/kit": "^2.50.1",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "prettier": "^3.8.1",
    "prettier-plugin-svelte": "^3.4.1",
    "rimraf": "^6.1.2",
    "svelte": "^5.48.2",
    "svelte-check": "^4.3.5",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  },
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "deploy:dev": "vite build && ow dev workers upload httpbin ./build",
    "deploy:local": "vite build && ow local workers upload httpbin ./build",
    "deploy:main": "vite build && ow main workers upload httpbin ./build",
    "clean": "rimraf build node_modules .svelte-kit .vercel .wrangler .netlify dist"
  },
  "type": "module"
}
